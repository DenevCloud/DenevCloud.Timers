<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net8.0;</TargetFrameworks>
    <version>0.1.2</version>
    <AssemblyVersion>0.1.3.8780</AssemblyVersion>
    <FileVersion>0.1.3.8780</FileVersion>
    <Authors>Milen Denev</Authors>
    <Company>Denev Cloud</Company>
    <Description>
      Simple timer with a trigger event and a handling event
      Example - https://github.com/DenevCloud/DenevCloud.Timers/tree/master/DenevCloud.Timers.Example
    </Description>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <RepositoryUrl>https://github.com/DenevCloud/DenevCloud.Timers</RepositoryUrl>
    <PackageProjectUrl>https://github.com/DenevCloud/DenevCloud.Timers</PackageProjectUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
